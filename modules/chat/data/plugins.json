{
  "metadata": {
    "description": "Complete list of all plugins available in WildMind AI Canvas with their categories, models, parameters, and capabilities",
    "version": "1.0.0",
    "lastUpdated": "2025-01-23",
    "source": "plugins directory analysis"
  },
  "features": {
    "imageEnhancement": {
      "name": "Image Enhancement",
      "description": "Enhance, upscale, and improve image quality",
      "supported": true,
      "pluginsCount": 3,
      "useCase": "Improve image resolution, quality, and details"
    },
    "imageEditing": {
      "name": "Image Editing",
      "description": "Edit, modify, and transform images",
      "supported": true,
      "pluginsCount": 3,
      "useCase": "Remove backgrounds, erase objects, expand images"
    },
    "imageConversion": {
      "name": "Image Conversion",
      "description": "Convert images to different formats and styles",
      "supported": true,
      "pluginsCount": 1,
      "useCase": "Vectorize images, convert to SVG"
    },
    "cameraControl": {
      "name": "Camera Control",
      "description": "Generate multiple camera angles and perspectives",
      "supported": true,
      "pluginsCount": 1,
      "useCase": "Create product shots from different angles, human poses"
    },
    "sceneGeneration": {
      "name": "Scene Generation",
      "description": "Generate storyboards and scene continuations",
      "supported": true,
      "pluginsCount": 2,
      "useCase": "Create storyboards from scripts, generate next scene images"
    }
  },
  "categories": [
    {
      "id": "image-enhancement",
      "name": "Image Enhancement",
      "description": "Upscale and enhance image quality",
      "plugins": [
        {
          "id": "upscale",
          "name": "Upscale",
          "description": "Increase image resolution and quality",
          "inputType": "image",
          "outputType": "image",
          "requiresSourceImage": true,
          "models": [
            {
              "id": "crystal-upscaler",
              "name": "Crystal Upscaler",
              "provider": "Replicate",
              "description": "High-quality upscaling with multiple resolution options",
              "parameters": {
                "scale": {
                  "type": "number",
                  "min": 1,
                  "max": 6,
                  "default": 2,
                  "description": "Upscale factor (1x to 6x)"
                },
                "faceEnhance": {
                  "type": "boolean",
                  "default": false,
                  "description": "Enable face enhancement (not supported for Crystal)"
                },
                "faceEnhanceStrength": {
                  "type": "number",
                  "min": 0,
                  "max": 1,
                  "default": 0.8,
                  "description": "Face enhancement strength (0-1, not applicable for Crystal)"
                },
                "topazModel": {
                  "type": "string",
                  "default": null,
                  "description": "Topaz model variant (only for Topaz Upscaler)"
                },
                "faceEnhanceCreativity": {
                  "type": "number",
                  "min": 0,
                  "max": 1,
                  "default": 0,
                  "description": "Face enhancement creativity (only for Topaz)"
                }
              },
              "supportedResolutions": ["1080p", "1440p", "2160p", "6K", "8K", "12K"],
              "credits": {
                "1080p": 220,
                "1440p": 420,
                "2160p": 820,
                "6K": 1620,
                "8K": 3220,
                "12K": 6420
              }
            },
            {
              "id": "topaz-upscaler",
              "name": "Topaz Upscaler",
              "provider": "FAL",
              "description": "Professional upscaling with face enhancement and multiple model variants",
              "parameters": {
                "scale": {
                  "type": "number",
                  "min": 1,
                  "max": 4,
                  "default": 2,
                  "description": "Upscale factor (1x to 4x max for Topaz)"
                },
                "faceEnhance": {
                  "type": "boolean",
                  "default": true,
                  "description": "Enable face enhancement"
                },
                "faceEnhanceStrength": {
                  "type": "number",
                  "min": 0,
                  "max": 1,
                  "default": 0.8,
                  "description": "Face enhancement strength (0-1)"
                },
                "topazModel": {
                  "type": "string",
                  "options": ["Default", "Low Resolution V2", "Standard V2", "CGI", "High Fidelity V2", "Text Refine", "Recovery", "Redefine", "Recovery V2"],
                  "default": "Standard V2",
                  "description": "Topaz model variant"
                },
                "faceEnhanceCreativity": {
                  "type": "number",
                  "min": 0,
                  "max": 1,
                  "default": 0,
                  "description": "Face enhancement creativity (0-1)"
                }
              },
              "supportedResolutions": ["24MP", "48MP", "96MP", "512MP"],
              "credits": {
                "24MP": 180,
                "48MP": 340,
                "96MP": 660,
                "512MP": 2740
              }
            },
            {
              "id": "real-esrgan",
              "name": "Real-ESRGAN",
              "provider": "Replicate",
              "description": "Fast and efficient upscaling with face enhancement support",
              "parameters": {
                "scale": {
                  "type": "number",
                  "min": 1,
                  "max": 6,
                  "default": 2,
                  "description": "Upscale factor (1x to 6x)"
                },
                "faceEnhance": {
                  "type": "boolean",
                  "default": true,
                  "description": "Enable face enhancement"
                },
                "faceEnhanceStrength": {
                  "type": "number",
                  "min": 0,
                  "max": 1,
                  "default": 0.8,
                  "description": "Face enhancement strength (0-1)"
                },
                "topazModel": {
                  "type": "string",
                  "default": null,
                  "description": "Not applicable for Real-ESRGAN"
                },
                "faceEnhanceCreativity": {
                  "type": "number",
                  "default": 0,
                  "description": "Not applicable for Real-ESRGAN"
                }
              },
              "supportedResolutions": ["1080p", "1440p", "2160p"],
              "credits": {
                "1080p": 180,
                "1440p": 340,
                "2160p": 660
              }
            }
          ],
          "defaultModel": "crystal-upscaler"
        }
      ]
    },
    {
      "id": "image-editing",
      "name": "Image Editing",
      "description": "Edit and modify images",
      "plugins": [
        {
          "id": "remove-bg",
          "name": "Remove Background",
          "description": "Remove or replace image backgrounds",
          "inputType": "image",
          "outputType": "image",
          "requiresSourceImage": true,
          "models": [
            {
              "id": "851-labs-background-remover",
              "name": "851-labs/background-remover",
              "provider": "Replicate",
              "description": "High-quality background removal",
              "parameters": {
                "backgroundType": {
                  "type": "string",
                  "options": ["green", "rgba (transparent)", "white", "blue", "overlay", "map"],
                  "default": "rgba (transparent)",
                  "description": "Background replacement type"
                },
                "scale": {
                  "type": "number",
                  "min": 0.1,
                  "max": 1,
                  "default": 0.5,
                  "description": "Scale factor for processing"
                }
              },
              "supportedResolutions": ["original", "1080p", "1440p", "2160p"],
              "credits": {
                "default": 100
              }
            }
          ],
          "defaultModel": "851-labs-background-remover"
        },
        {
          "id": "erase",
          "name": "Erase",
          "description": "Erase objects or parts of images using masks",
          "inputType": "image",
          "outputType": "image",
          "requiresSourceImage": true,
          "models": [
            {
              "id": "bria-eraser",
              "name": "bria/eraser",
              "provider": "Bria",
              "description": "AI-powered object erasure with mask support",
              "parameters": {
                "mask": {
                  "type": "string",
                  "format": "base64_image",
                  "description": "Mask image (white areas = erase, black = keep)"
                },
                "prompt": {
                  "type": "string",
                  "default": "",
                  "description": "Optional prompt describing what to erase"
                },
                "brushSize": {
                  "type": "number",
                  "min": 1,
                  "max": 100,
                  "default": 20,
                  "description": "Brush size for mask drawing"
                }
              },
              "supportedResolutions": ["original"],
              "credits": {
                "default": 80
              }
            }
          ],
          "defaultModel": "bria-eraser"
        },
        {
          "id": "expand",
          "name": "Expand",
          "description": "Expand image boundaries and fill new areas",
          "inputType": "image",
          "outputType": "image",
          "requiresSourceImage": true,
          "models": [
            {
              "id": "bria-expander",
              "name": "bria/expander",
              "provider": "Bria",
              "description": "Intelligent image expansion with prompt guidance",
              "parameters": {
                "prompt": {
                  "type": "string",
                  "default": "",
                  "description": "Description of what to generate in expanded areas"
                },
                "aspectRatio": {
                  "type": "string",
                  "options": ["1:1", "2:3", "3:2", "3:4", "4:3", "4:5", "5:4", "9:16", "16:9", "custom"],
                  "default": "1:1",
                  "description": "Target aspect ratio for expanded image"
                },
                "canvasSize": {
                  "type": "array",
                  "items": "number",
                  "format": "[width, height]",
                  "description": "Custom canvas size (when aspectRatio is 'custom')"
                },
                "originalImageSize": {
                  "type": "array",
                  "items": "number",
                  "format": "[width, height]",
                  "description": "Original image dimensions"
                },
                "originalImageLocation": {
                  "type": "array",
                  "items": "number",
                  "format": "[x, y]",
                  "description": "Original image position on canvas"
                }
              },
              "supportedResolutions": ["1080p", "1440p", "2160p"],
              "credits": {
                "default": 100
              }
            }
          ],
          "defaultModel": "bria-expander"
        }
      ]
    },
    {
      "id": "image-conversion",
      "name": "Image Conversion",
      "description": "Convert images to different formats",
      "plugins": [
        {
          "id": "vectorize",
          "name": "Vectorize",
          "description": "Convert raster images to vector format (SVG)",
          "inputType": "image",
          "outputType": "svg",
          "requiresSourceImage": true,
          "models": [
            {
              "id": "recraft-vectorize",
              "name": "Recraft",
              "provider": "Recraft",
              "description": "Convert images to scalable vector graphics",
              "parameters": {
                "mode": {
                  "type": "string",
                  "options": ["simple", "Detailed"],
                  "default": "simple",
                  "description": "Vectorization mode: simple (faster, less detail) or Detailed (slower, more detail)"
                }
              },
              "supportedResolutions": ["original"],
              "credits": {
                "simple": 40,
                "Detailed": 60
              }
            }
          ],
          "defaultModel": "recraft-vectorize"
        }
      ]
    },
