        }
      ]
    },
    {
      "id": "scene-generation",
      "name": "Scene Generation",
      "description": "Generate storyboards and scene continuations",
      "plugins": [
        {
          "id": "storyboard",
          "name": "Storyboard",
          "description": "Generate storyboard images from script text and character/background descriptions",
          "inputType": "text",
          "outputType": "image",
          "requiresSourceImage": false,
          "models": [
            {
              "id": "storyboard-generator",
              "name": "Storyboard Generator",
              "provider": "Internal",
              "description": "Generate storyboard frames from script and descriptions",
              "parameters": {
                "scriptText": {
                  "type": "string",
                  "required": true,
                  "description": "Script text describing scenes"
                },
                "characterInput": {
                  "type": "string",
                  "default": "",
                  "description": "Character descriptions"
                },
                "characterNames": {
                  "type": "string",
                  "default": "",
                  "description": "Comma-separated character names"
                },
                "backgroundDescription": {
                  "type": "string",
                  "default": "",
                  "description": "Background/setting descriptions"
                },
                "specialRequest": {
                  "type": "string",
                  "default": "",
                  "description": "Additional special requests or style notes"
                },
                "characterNamesMap": {
                  "type": "object",
                  "format": "Record<number, string>",
                  "description": "Map of scene number to character name"
                },
                "propsNamesMap": {
                  "type": "object",
                  "format": "Record<number, string>",
                  "description": "Map of scene number to props"
                },
                "backgroundNamesMap": {
                  "type": "object",
                  "format": "Record<number, string>",
                  "description": "Map of scene number to background"
                }
              },
              "supportedResolutions": ["1080p", "1440p"],
              "credits": {
                "default": 200
              }
            }
          ],
          "defaultModel": "storyboard-generator"
        },
        {
          "id": "next-scene",
          "name": "Next Scene",
          "description": "Generate the next scene image based on previous scene and prompt",
          "inputType": "image",
          "outputType": "image",
          "requiresSourceImage": true,
          "models": [
            {
              "id": "next-scene-generator",
              "name": "Next Scene Generator",
              "provider": "Internal",
              "description": "Generate continuation scene from previous image",
              "parameters": {
                "mode": {
                  "type": "string",
                  "options": ["scene"],
                  "default": "scene",
                  "description": "Generation mode"
                },
                "prompt": {
                  "type": "string",
                  "default": "",
                  "description": "Description of the next scene"
                },
                "aspectRatio": {
                  "type": "string",
                  "options": ["1:1", "16:9", "9:16", "4:3", "3:4"],
                  "default": "1:1",
                  "description": "Output aspect ratio"
                },
                "loraScale": {
                  "type": "number",
                  "min": 0,
                  "max": 2,
                  "default": 1.15,
                  "description": "LoRA scale for consistency (0-2)"
                }
              },
              "supportedResolutions": ["1080p", "1440p"],
              "credits": {
                "default": 150
              }
            }
          ],
          "defaultModel": "next-scene-generator"
        }
      ]
    }
  ]
}
